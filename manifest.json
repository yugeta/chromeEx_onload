{
	"name": "GmailSendChecker",
	"version": "1.1",
	"manifest_version": 2,

	"description": "GmailSendChecker",

	"icons": {
		"16" : "contents/img/icon.png",
		"48" : "contents/img/icon.png",
		"128": "contents/img/icon_128.png"
	},

	"browser_action":{
		"default_icon":"contents/img/icon.png",
		"default_title":"navicast_bookmarklet",
		"default_popup":"contents/popup.html"
	},

	"content_scripts":[{
		"matches":[
			"http://*/*",
			"https://*/*"
		],
		"js":[
			"contents/js/lib.js",
			"contents/js/load.js",
			"contents/js/init.js"
		]
	}]
}
